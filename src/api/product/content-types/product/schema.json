{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "tshirt",
        "mug",
        "shoes",
        "table",
        "dress",
        "wall paper"
      ],
      "default": "wall paper",
      "required": true
    },
    "size": {
      "type": "string"
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "red",
        "black",
        "white",
        "brown",
        "green",
        "blue",
        "pink"
      ],
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "availableQty": {
      "type": "integer",
      "required": true
    }
  }
}
